  },
  "homepage": "https://github.com/takashi1kun/lan-play-gui-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.0",
    "@types/portscanner": "^2.1.0",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@now/node-server": "^0.8.2",
